<h1><img class="logo" src="/image/cat_circuit.png"><br> Gestion des Circuits </h1>
<a class="ajouter" href="/ajouterCircuit/"><p><img src="/image/act_add.png">  Ajouter un circuit</p></a>

<table>
      <tr>
        <th>Nom</th>
        <th>Longueur</th>
        <th>Nombre de sp√©ctateurs</th>
        <th>Actions</th>
      </tr>
      {{#each listeCircuit}}
        <tr>
            <td>{{this.nom}}</td>
            <td>{{this.longu}}</td>
            <td>{{this.nbrSpec}} </td>
            <td><div class="modsup">
              <div class="mod">
                <a href="/modifierCircuit/{{this.num}}"><img src="/image/act_edit.png"><p>Modifier</p></a>
              </div>
                <div class="sup">
              <a href="/supprimerCircuit/{{this.num}}{{#ifCond gpnum '==' NULL}}{{else}}/{{gpnum}}{{/ifCond}}"><img src="/image/act_delete.png"><p>Supprimer</p></a>
            </div>
          </td>
          </div>
        </tr>
      {{/each}}
</table>

